[program:backend_{{ api_environment }}]
directory={{ api_files_location }}
command=babel ./src -d ./dist && node ./dist/index.js
autostart=true
autorestart=true
stderr_logfile=/var/log/{{ api_url }}.err.log
stdout_logfile=/var/log/{{ api_url }}.out.log
environment=NODE_ENV="{{ api_environment }}",DATABASE_URL="{{ api_db_url }}"